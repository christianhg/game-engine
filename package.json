{
  "private": true,
  "scripts": {
    "dev": "trash .cache && parcel --no-hmr index.html",
    "dist": "trash dist && npm run prettier-production && parcel build --experimental-scope-hoisting index.html",
    "prettier": "prettier --write {*,**/*}.{css,js}",
    "prettier-production": "prettier --list-different {*,**/*}.{css,js}"
  },
  "babel": {
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "modern-normalize": "^0.5.0",
    "parcel-bundler": "^1.9.4",
    "prettier": "^1.13.7",
    "ramda": "^0.25.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "trash-cli": "^1.4.0"
  }
}
